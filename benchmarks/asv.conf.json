{
    "version": 1,
    "project": "holonote",
    "project_url": "https://github.com/holoviz/holonote",
    "repo": "..",
    "dvcs": "git",
    "repo_subdir": "",
    "branches": ["main"],
    "install_command": ["in-dir={env_dir} python -m pip install {wheel_file}"],
    "uninstall_command": ["return-code=any python -m pip uninstall -y {project}"],
    "build_command": [
        "python -m pip install hatch hatch-vcs",
        "PIP_NO_BUILD_ISOLATION=false python -mpip wheel --no-deps --no-index -w {build_cache_dir} {build_dir}"
    ],
    "environment_type": "virtualenv",
    "install_timeout": 600,
    "show_commit_url": "https://github.com/holoviz/holonote/commit/",
    "benchmark_dir": "benchmarks",
    "env_dir": ".asv/env",
    "results_dir": ".asv/results",
    "html_dir": ".asv/html",
    "hash_length": 8,
    "build_cache_size": 2
}
